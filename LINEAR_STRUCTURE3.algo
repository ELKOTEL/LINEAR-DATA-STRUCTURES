//Problem 2, Solution 1 with an array.
ALGORITHM sum_overlapping_elements
VAR
    arr1 : ARRAY_OF INTEGER[length];
    arr2 : ARRAY_OF INTEGER[length];
    sum:INTEGER := 0;

BEGIN
    Read(arr1)
    Read(arr2)
    FOR i FROM 0 TO arr1-1 STEP step  DO
        IF (arr1[i] in arr2) THEN
            sum := sum +arr1[i]
        ELSE
            sum := sum
        END_IF
    END_FOR


    Write (sum) 
END