//Problem 1, Solution 2 with an hash table.
ALGORITHM sum_distinct_elements
VAR
    arr1 : ARRAY_OF INTEGER[length];
    arr2 : ARRAY_OF INTEGER[length];
    htab: HASH_TABLE<INTEGER,INTEGER>
BEGIN
    Read(arr1)
    Read(arr2)
    FOR i FROM 0 TO arr1.length STEP 1  DO
        key=arr1[i];
        count=i;
        htab.insert(key,count);
    END_FOR

    FOR i FROM 0 TO arr2.length STEP 1  DO
        htab.insert(arr2[i],count);
    END_FOR







END